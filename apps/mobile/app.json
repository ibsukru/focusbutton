{
  "expo": {
    "name": "FocusButton",
    "slug": "focusbutton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.focusbutton.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.focusbutton.app",
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "USE_FULL_SCREEN_INTENT"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#000000",
          "sounds": ["./assets/sounds/timer_end.mp3"],
          "mode": "production",
          "androidMode": "default",
          "androidCollapsedTitle": "FocusButton",
          "androidImportance": "max",
          "androidShowBadge": true,
          "androidNotificationIcon": "./assets/images/icon.png",
          "androidShouldShowAlert": true,
          "androidShouldPlaySound": true,
          "androidShouldVibrate": true
        }
      ],
      "expo-secure-store"
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "extra": {
      "eas": {
        "projectId": "be0cf6d8-dfe3-411d-b54d-ad6b922bb366"
      }
    }
  }
}
